@model IEnumerable<SGAIJE.Models.CatalogoArticulo>

@{
    ViewBag.Title = "Articulos";
    Layout = "~/Views/Shared/_LayoutSGAIJE.cshtml";
}

<script>
    var DsArticulo = @Html.Raw(Json.Encode(Model).ToString())
</script>

<div class="modal fade bd-example" id="mdlCrudArticulos" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog " role="document">
        <div class="modal-content">
            <div class="modal-header" style="background-color:#dd4b39">
                <img src='@Url.Content("~/Content/SGAIJE.jpeg")' width="80" align="left">
                <h2 class="modal-title" id="mdlcrudtitleArticulos" style="color:white"></h2>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="mdlContentCrudArticulos">

                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        <button type="button" class="btn btn-primary" onclick="Alta()">Agregar</button>
    </div>
</div>
<div id="Current">

</div>

<div class="dx-viewport">
    <div class="demo-container">
        <div id="gridArticulos"></div>
    </div>
</div>

<script src="~/Scripts/CrudGen/CrudCatalogoArticulo.js"></script>



