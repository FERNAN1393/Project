    @{
        if (Session["Message"] != null)
        {
            <script>
        var tipo = "@Session["MessageTipo"].ToString();".slice(0, -1)
        console.log(tipo);
                swal({
                    title: "@Session["Message"].ToString();",
                    text: "@Session["MessageDatos"].ToString();",
                    type: tipo
                })
            </script>
            Session["Message"] = null;
            Session["MessageDatos"] = null;
            Session["MessageTipo"] = null;
        }
    }
